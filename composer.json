{
    "name": "hereldar/doctrine-mapping",
    "type": "library",
    "description": "An alternative Doctrine object mapper.",
    "keywords": ["doctrine", "orm", "persistence", "mapper"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hereldar/doctrine-mapping/issues",
        "source": "https://github.com/hereldar/doctrine-mapping"
    },
    "authors": [
        {
            "name": "Samuel Maudo",
            "email": "samuelmaudo@gmail.com",
            "homepage": "https://github.com/samuelmaudo"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/orm": "^2.18|^3.0|^4.0"
    },
    "require-dev": {
        "composer/semver": "^3.4",
        "hereldar/coding-style": "^0.1.2",
        "hereldar/faker-helper": "^0.1.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-phpunit": "^0.19.0",
        "vimeo/psalm": "^5.23.0"
    },
    "autoload": {
        "psr-4": {
            "Hereldar\\DoctrineMapping\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hereldar\\DoctrineMapping\\Tests\\": "tests/"
        },
        "files": [
          "src/Internals/functions.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
